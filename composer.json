{
  "name": "medology/gherkin-cs-fixer",
  "description": "Gherkin code auto formatter",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Medology\\GherkinCsFixer\\": "src/"
    }
  },
  "bin": [
    "bin/gherkin-fixer"
  ],
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "medology/custom-php-cs-fixer": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:Medology/custom-php-cs-fixer.git"
    }
  ]
}
